{
    "compilerOptions": {
      "outDir": "./build/",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "allowJs": true,
      "noEmitOnError": true,
      "moduleResolution": "node",
      "typeRoots": [ "node_modules/@types" ],
      "strict": false,        
      "esModuleInterop": true      
    }
}



